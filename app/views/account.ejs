        }
    
        .cart-sidebar {
            position: fixed;
            top: 0;
            right: -100%;
            width: 350px;
            max-width: 100%;
            height: 100vh;
            background: #191919;
            box-shadow: -2px 0 10px rgba(0, 0, 0, 0.6);
