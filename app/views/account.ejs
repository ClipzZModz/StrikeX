    <script src="https://cdn.jsdelivr.net/npm/notyf@3/notyf.min.js"></script>
    <script>
        const notyf = new Notyf({
          duration: 3000,
          position: { x: 'left', y: 'bottom' },
          types: [
            {
              type: 'warning',
              background: 'orange',
              icon: {
                className: 'material-icons',
                tagName: 'i',
                text: 'warning',
              },
            },
          ],
        });
      
        function notify(type, message = '') {
          switch (type) {
            case 'success':
              notyf.success(message || 'Product has been added to the cart!');
              break;
            case 'warning':
              notyf.open({
                type: 'warning',
                message: message || 'There was a problem completing this request.',
              });
              break;
            case 'danger':
              notyf.error(message || 'There was an error adding this product to the cart.');
              break;
            default:
              notyf.open({
                type: 'warning',
                message: message || 'Unknown Error!',
              });
              break;
          }
        }
      
        const cartItemsFromServer = <%- JSON.stringify(cartItems) %>;
      
        const cartButton = document.getElementById('cart-section');
        const cartSidebar = document.getElementById('cartSidebar');
        const closeCartBtn = document.getElementById('closeCart');
        const cartItemsContainer = document.getElementById('cartItems');
        const cartTotal = document.getElementById('cartTotal');
        const buttonTotal = document.getElementById('cart-price');
        const buttonQTY = document.getElementById('cart-count');
      
        let cart = [];
      
        const renderCart = async () => {
          let currencyCode = 'GBP';
          cartItemsContainer.innerHTML = '';
          let total = 0;
      
          for (const { name, price, img, qty } of cart) {
            let finalPrice = typeof price === 'number' ? price : 0;
            let finalQty = typeof qty === 'number' ? qty : 1;
            
            const item = document.createElement('div');
            item.className = 'cart-item';
      
            try {
              const response = await fetch(`/checkout/product/${encodeURIComponent(name)}`);
              if (!response.ok) throw new Error(`HTTP error! Status: ${response.status}`);
      
              const result = await response.json();
              const priceAmount = parseFloat(JSON.parse(result.productData[0].uk_price_obj).price.amount);
      
              item.innerHTML = `
                <img src="${img}" alt="${name}" />
                <div class="cart-item-details">
                  <strong>${name}</strong><br>
                  &pound;${priceAmount.toFixed(2)} GBP x ${finalQty}
                </div>
              `;
      
              buttonQTY.innerHTML = result.totalCartItems;
              total += priceAmount * finalQty;
              cartItemsContainer.appendChild(item);
            } catch (error) {
              console.error('Fetch error:', error);
            }
          }
      
          cartTotal.textContent = `\u00a3${total.toFixed(2)}`;
          buttonTotal.textContent = `\u00a3${total.toFixed(2)}`;
        };
      
        const toggleCart = (show = false) => {
          cartSidebar.classList.toggle('open', show);
        };
      
        const addItemToCart = (cartData) => {
          if (!cartData?.lines?.edges?.length) {
            console.error('Invalid cart data:', cartData);
            return;
          }
      
          cart = cartData.lines.edges.map((edge) => {
            const item = edge.node;
            return {
              name: item.title || 'Unnamed Product',
              price: parseFloat(item.price) || 0,
              qty: parseInt(item.quantity) || 1,
              img: item.image || 'assets/images/default.jpg',
            };
          });
      
          renderCart();
          toggleCart(true);
        };
      
        const addToCart = async (variantId, quantity) => {
          try {
            const qty = parseInt(document.getElementById('product-quantity')?.value, 10);
      
            console.log(qty)

            const response = await fetch('/api/v1/cart/add', {
              method: 'POST',
              headers: { 'Content-Type': 'application/json' },
              body: JSON.stringify({ variantId, quantity: qty }),
            });
      
            const result = await response.json();
      
            if (response.ok) {
              addItemToCart(result.cart);
              notify('success');
            } else {
              notify('danger', 'Failed to add item to cart');
            }
          } catch (error) {
            notify('danger', 'An error occurred');
          }
        };
      
        document.addEventListener('DOMContentLoaded', async () => {
          console.log('Cart items loaded from server:', cartItemsFromServer);
      
          // Just render the cart on page load — don't call addToCart
          cart = cartItemsFromServer.map((item) => ({
            name: item.title || 'Unnamed Product',
            price: parseFloat(item.price) || 0,
            qty: parseInt(item.quantity) || 1,
            img: item.image || 'assets/images/default.jpg',
          }));
      
          renderCart();
      
          // Button toggle listeners
          cartButton.addEventListener('click', () => toggleCart(true));
          closeCartBtn.addEventListener('click', () => toggleCart(false));
      
          // Add to cart button events
          document.querySelectorAll('.add-to-cart').forEach((button) => {
            button.addEventListener('click', async function () {
              const productId = this.getAttribute('data-product-id');
              const quantity = 1;
      
              try {
                await addToCart(productId, quantity);
      
                cartItemsFromServer.push({
                  merchandiseId: productId,
                  quantity,
                  title: this.getAttribute('data-title'),
                  image: this.getAttribute('data-image'),
                });
              } catch (error) {
                notify('danger');
              }
            });
          });
        });
    </script>

