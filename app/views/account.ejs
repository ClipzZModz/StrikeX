                  </div>
                </div>

                <!-- Orders Tab -->
                <div class="tab-pane fade" id="orders" role="tabpanel" aria-labelledby="orders-tab">
                  <div class="mb-4" style="margin-top: 3%">
                    <h3 style="color: #fff;">Your Orders</h3>
                    <p style="color: #ccc;">View all your recent orders below.</p>
                  </div>
                  <% if (orders.length > 0) { %>
                    <table class="table table-dark">
                      <thead>
                        <tr>
                          <th>Order #</th>
                          <th>Date</th>
                          <th>Total</th>
                          <th>Status</th>
                          <th></th>
                        </tr>
                      </thead>
                      <tbody>
                        <% orders.forEach(order => { %>
                          <tr>
                            <td><%= order.id %></td>
                            <td><%= moment(order.date).format('MMMM Do YYYY') %></td>
                            <td>&pound;<%= parseInt(order.total).toFixed(2) %> GBP</td>
                            <td><%= order.status %></td>
                            <td>
                              <a href="/account/order/<%= order.id %>" style="color: #fa4f09;">View</a>
                            </td>
                          </tr>
                        <% }) %>
                      </tbody>
                    </table>
                  <% } else { %>
                    <p style="color: #ccc;">No orders yet. Start shopping for your fishing gear!</p>
                  <% } %>
                </div>

                <!-- Wishlist Tab -->
                <div class="tab-pane fade" id="wishlist" role="tabpanel" aria-labelledby="wishlist-tab">
                  <div class="mb-4" style="margin-top: 3%">
                    <h3 style="color: #fff;">Your Wishlist</h3>
                    <p style="color: #ccc;">View all your saved items below.</p>
                  </div>
                  <% if (wishlist.length > 0) { %>
                    <table class="table table-dark">
                      <thead>
                        <tr>
                          <th>Order #</th>
                          <th>Date</th>
                          <th>Total</th>
                          <th>Status</th>
                          <th></th>
                        </tr>
                      </thead>
                      <tbody>
                        <% wishlist.forEach(wishlist => { %>
                          <tr>
                            <td><%= wishlist.id %></td>
                            <td><%= wishlist.date %></td>
                            <td><%= wishlist.total %></td>
                            <td><%= wishlist.status %></td>
                            <td>
                              <a href="/account/order/<%= wishlist.id %>" style="color: #fa4f09;">View</a>
                            </td>
                          </tr>
                        <% }) %>
                      </tbody>
                    </table>
                  <% } else { %>
                    <p style="color: #ccc;">There is nothing in your wishlist yet.</p>
                  <% } %>
                </div>

                <!-- Account Details Tab -->
                <div class="tab-pane fade" id="details" role="tabpanel" aria-labelledby="details-tab">
                  <div class="mb-4" style="margin-top: 3%">
                    <h3 style="color: #fff;">Account Details</h3>
                    <p style="color: #ccc;">Manage your account information below.</p>
                  </div>
                  <!-- Placeholder content; replace with actual account details form or data -->
                  <p style="color: #ccc;">[Account details form or data goes here]</p>
                </div>
              </div>
            </div>
          </section>
        <!-- Account Overview Area End -->
    </main>

    <!-- Bootstrap min Js -->
    <script src="/assets/js/bootstrap.min.js"></script>
    <!-- All min Js -->
    <script src="/assets/js/all.min.js"></script>
</body>

</html>


