    <script src="https://cdn.jsdelivr.net/npm/notyf@3/notyf.min.js"></script>
    <script>
        const notyf = new Notyf({
          duration: 3000,
          position: { x: 'left', y: 'bottom' },
          types: [
            {
              type: 'warning',
              background: 'orange',
              icon: {
                className: 'material-icons',
                tagName: 'i',
                text: 'warning',
              },
            },
          ],
        });
      
        function notify(type, message = '') {
          switch (type) {
            case 'success':
              notyf.success(message || 'Product has been added to the cart!');
              break;
            case 'warning':
              notyf.open({
                type: 'warning',
                message: message || 'There was a problem completing this request.',
              });
              break;
            case 'danger':
              notyf.error(message || 'There was an error adding this product to the cart.');
              break;
            default:
              notyf.open({
                type: 'warning',
                message: message || 'Unknown Error!',
              });
              break;
          }
        }
      
                window.cartItemsFromServer = <%- JSON.stringify(cartItems) %>;

        document.addEventListener('DOMContentLoaded', () => {
          document.querySelectorAll('.add-to-cart').forEach((button) => {
            button.addEventListener('click', async function () {
              const productId = this.getAttribute('data-product-id');
              const quantity = parseInt(document.getElementById('quantity')?.value, 10) || 1;

              if (!window.addToCart) {
                return;
              }

              try {
                await window.addToCart(productId, quantity);
              } catch (error) {
                console.error('Add to cart error:', error);
              }
            });
          });
        });</script>

      <% if(cartItems.length > 0) { %>
    
        <script>toggleCart(true);</script>
        
      <% } %>
      

